{
  "name": "thoughtly",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "rimraf build && ./node_modules/.bin/tsc",
    "debug": "NODE_OPTIONS='--trace-warnings --inspect' ts-node-dev --respawn --transpile-only src/index.ts",
    "dev": "NODE_OPTIONS='--trace-warnings' ts-node-dev src/index.ts",
    "lint": "./node_modules/.bin/eslint src --ext ts --fix",
    "lint-staged": "./node_modules/.bin/lint-staged",
    "postinstall": "yarn run setup-chrome && cd .. && husky install",
    "deploy": "sh ./scripts/deploy.sh",
    "deploy:staging": "sh ./scripts/deploy-staging.sh",
    "email": "email dev -d ./src/services/email/emails",
    "ssh-vm": "gcloud compute ssh --zone \"us-east4-c\" \"async-compute\" --project \"thoughtlead\"",
    "setup-chrome": "node node_modules/puppeteer/install.mjs",
    "stripe": "stripe listen --forward-to localhost:8080/webhook/stripe",
    "typeorm": "typeorm-ts-node-commonjs",
    "db:migrate": "yarn typeorm migration:run -d ./src/services/database/data-source.ts",
    "db:revert": "yarn typeorm migration:revert -d ./src/services/database/data-source.ts",
    "db:migrate:generate": "yarn typeorm migration:generate -d ./src/services/database/data-source.ts ./src/migrations/ChangeMe",
    "dev:test": "TEST=true ts-node-dev src/index.ts",
    "test:dev": "TEST=true concurrently \"yarn dev\" \"wait-on http://localhost:8080 && yarn test\"",
    "test": "dotenv_config_path=./.env.test jest --forceExit --detectOpenHandles"
  },
  "author": "",
  "license": "ISC",
